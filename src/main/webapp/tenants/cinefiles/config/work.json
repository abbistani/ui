{
	"pageBuilder": {
		"options": {
			"pageType": "work",
			"components": {
				"recordEditor": {
					"type":	 "cspace.recordEditor",
					"options": {
						"uispec": "{pageBuilder}.options.uispec.recordEditor",
						"fieldsToIgnore": ["csid", "fields.csid", "fields.shortIdentifier", "fields.narrowerContexts", "fields.createdAt", "fields.createdBy", "fields.updatedAt", "fields.updatedBy"],
						"selectors": {
							"identificationNumber": ".csc-workAuthority-termName"
						}
					}
				},
				"titleBar": {
					"type": "cspace.titleBar",
					"options": {
						"fields": [{
							"type": "repeatableMatch",
							"queryPath": "fields.workTermGroup",
							"childPath": "_primary",
							"value": true,
							"path": "termDisplayName"
						}]
					}
				},
				"header": {
					"type": "cspace.header",
					"options": {
						"schema": "{pageBuilder}.schema",
						"permissions": "{pageBuilder}.permissions"
					}
				},
				"tabs": {
					"type": "cspace.tabs"
				},
				"sidebar": {
					"type": "cspace.sidebar"
				}
			}
		}
	},
	"pageBuilderIO": {
		"options": {
			"recordType": "work",
			"schema": {
				"work": null
			}
		}
	}
}
